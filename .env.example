# Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control

# Convex Configuration (Required)
# Get this from your Convex dashboard after running 'npx convex dev --configure'
VITE_CONVEX_URL=https://your-deployment-name.convex.cloud

# Xano Authentication Token
# Get this from your Xano dashboard
VITE_XANO_AUTH_TOKEN=your_xano_auth_token_here

# Mem API Configuration
# Get this from your Mem dashboard
VITE_MEM_API_KEY=your_mem_api_key_here

# Polygon.io API Configuration
# Get this from your Polygon.io dashboard
VITE_POLYGON_API_KEY=your_polygon_api_key_here

# Admin Configuration
# Comma-separated list of admin email addresses
VITE_ADMIN_EMAILS=msbaxter@gmail.com

# Add other environment variables as needed
# VITE_API_BASE_URL=https://api.example.com
# VITE_APP_NAME=Options Trading Dashboard
